{
    "name": "tasks-button",
    "displayName": "Tasks Button",
    "description": "Tasks Button，为你的 Tasks 添加 Button !",
    "version": "0.0.7",
    "icon": "icon.png",
    "publisher": "quzhen",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.vscode/tasks.json"
    ],
    "contributes": {
        "commands": [
            {
                "command": "tasks-button.refresh",
                "title": "刷新任务按钮",
                "category": "Tasks Button"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hellotrik/vscode-tasks-button.git"
      },
    "main": "./extension.js"
}